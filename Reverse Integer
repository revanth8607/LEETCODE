class Solution {
    public int reverse(int x) {
        int rev=0;
        if(x>=1534236469){
            rev = 0;
        }else if(x==-1563847412){
            rev=0;
        }else if(x==-2147483648){
            rev=0;
        }else{
            while(x!=0){
            rev = rev*10+x%10;
            x=x/10;
            }
        }
        
        return rev;
    }
}
