class Solution {
    public int[][] construct2DArray(int[] original, int m, int n) {
        int[][] arr = new int[m][n];
        int count=0;
        int[][] a = {};
        if(m*n!=original.length){
            return a;
        }else{
            for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                arr[i][j] = original[count];
                count++; 
            }
        }
        }
        
        return arr;
    }
}
