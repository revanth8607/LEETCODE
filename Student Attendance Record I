class Solution {
    public boolean checkRecord(String s) {
        int a =0;
        boolean t = false;
        boolean c = true;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='A'){
                a++;
            }
        }
        if(s.contains("LLL")){
            c = false;
        }
        if(a<2 && c==true){
            return true;
        }else{
            return false;
        }
    }
}
